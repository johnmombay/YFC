@model Web.YFC.Models.Content

<!-- subheader begin -->
<section id="subheader" data-speed="2" data-type="background">
    <div class="container">
        <div class="row">
            <div class="col-md-12">

                <h1>@ViewBag.Section</h1>
            </div>
        </div>
    </div>
</section>
<!-- subheader close -->

<div class="clearfix"></div>

<!-- content begin -->
<div id="content">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="blog-read">
                    <div>
                        <div class="preview">
                            @Html.Raw(Model.Material)
                        </div>
                    </div>
                </div>                
            </div>
        </div>
    </div>
</div>
<!-- content close -->